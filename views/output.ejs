<html lang="en-US">
<head>
    <title>Mock-N-Serve</title>
    <style>
        table {
            border: 0.5px solid #a0a0a0;
        }
        td {
            border: 0.5px solid #a0a0a0;
            padding: 5px;
        }
        td:first-of-type {
            vertical-align: top;
        }
    </style>
</head>
<body>
    <table cellspacing="0">
        <tbody>
        <tr>
            <td>Method</td>
            <td><%=method%></td>
        </tr>
        <tr>
            <td>URL</td>
            <td><%=url%></td>
        </tr>
        <tr>
            <td>Body</td>
            <td><pre><%=JSON.stringify(body, null, 4)%></pre></td>
        </tr>
        <tr>
            <td>Query</td>
            <td><pre><%=JSON.stringify(query, null, 4)%></pre></td>
        </tr>
        <tr>
            <td>Headers</td>
            <td><pre><%=JSON.stringify(headers, null, 4)%></pre></td>
        </tr>
        <tr>
            <td>Cookies</td>
            <td><pre><%=JSON.stringify(cookies, null, 4)%></pre></td>
        </tr>
        <tr>
            <td>Files</td>
            <td><pre><%=JSON.stringify(files, null, 4)%></pre></td>
        </tr>
        </tbody>
    </table>
</body>
</html>
