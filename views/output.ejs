<html lang="en-US">
<head>
    <title>Mock-N-Serve</title>
</head>
<body>
    <table>
        <tbody>
        <tr>
            <td>Method</td>
            <td><%=method%></td>
        </tr>
        <tr>
            <td>URL</td>
            <td><%=originalUrl%></td>
        </tr>
        <tr>
            <td>Body</td>
            <td><pre><%=JSON.stringify(body, null, 4)%></pre></td>
        </tr>
        <tr>
            <td>Query</td>
            <td><pre><%=JSON.stringify(query, null, 4)%></pre></td>
        </tr>
        <tr>
            <td>Params</td>
            <td><pre><%=JSON.stringify(params, null, 4)%></pre></td>
        </tr>
        <tr>
            <td>Cookies</td>
            <td><pre><%=JSON.stringify(cookies, null, 4)%></pre></td>
        </tr>
        </tbody>
    </table>
</body>
</html>
